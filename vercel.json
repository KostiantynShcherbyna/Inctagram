{
  "version": 2,
  "name": "inctagram",
  "builds": [
    { "src": "apps/main/src/main.ts", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "(.*)", "dest": "apps/main/src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH"]
    }
  ],
  "build": {
    "env": {
      "PRISMA_CLIENT_ENGINE_TYPE": "binary",
      "PRISMA_CLIENT_ENGINE_PATH": "./node_modules/.prisma/client"
    },
    "use": "@vercel/node"
  }
}