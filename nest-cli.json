{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/inctagram/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/inctagram/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/inctagram",
  "projects": {
    "inctagram": {
      "type": "application",
      "root": "apps/inctagram",
      "entryFile": "main",
      "sourceRoot": "apps/inctagram/src",
      "compilerOptions": {
        "tsConfigPath": "apps/inctagram/tsconfig.app.json"
      }
    },
    "monorepo": {
      "type": "application",
      "root": "apps/monorepo",
      "entryFile": "main",
      "sourceRoot": "apps/monorepo/src",
      "compilerOptions": {
        "tsConfigPath": "apps/monorepo/tsconfig.app.json"
      }
    }
  }
}